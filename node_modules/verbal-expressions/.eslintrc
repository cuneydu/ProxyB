{
  "extends": "airbnb/legacy",
  "rules": {
    // Override Airbnb's style guide defaults for ES5
    "indent": [2, 4],
    "max-len": [2, 255, 4],
    "no-param-reassign": 0,
    "spaced-comment": [2, "always", { "markers": ["/", "!"] }]
  },
  "env": {
    "amd": true,
    "qunit": true
  },
  "globals": {
    "VerEx": true
  }
}
